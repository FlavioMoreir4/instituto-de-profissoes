{
  "name": "Instituto_de_Profissoes",
  "productName": "Instituto de Profiss천es",
  "version": "1.0.0",
  "description": "Aula Interativa",
  "private": true,
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --debug",
    "test": "mocha && standard",
    "package": "npm-run-all package:*",
    "package:mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --out=out --icon=assets/app-icon/mac/app.icns --osx-sign.identity='Developer ID Application: GitHub' --extend-info=assets/mac/info.plist",
    "package:win": "electron-packager . --overwrite --platform=win32 --arch=ia32 --out=out --icon=assets/app-icon/win/app.ico",
    "package:linux": "electron-packager . --overwrite --platform=linux --arch=x64 --out=out",
    "package:sign-exe": "signcode './out/Instituto de Profiss천esSetup-win32-ia32/aula.exe' --cert ~/Instituto_de_Profissoes.p12 --prompt --name 'Instituto de Profiss천es' --url 'http://iprofissoes.com.br/'",
    "package:installer": "node ./script/installer.js",
    "package:sign-installer": "signcode './out/windows-installer/AulaSetup.exe' --cert ~/Instituto_de_Profissoes.p12 --prompt --name 'Instituto de Profiss천es' --url 'http://iprofissoes.com.br/'",
    "package:mas": "./script/mas.sh",
    "windows-store": "node ./script/windows-store.js",
    "release": "node ./script/release.js",
    "prepack": "check-for-leaks",
    "prepush": "check-for-leaks"
  },
  "repository": "https://github.com/FlavioMoreiraTec/instituto-de-profissoes",
  "keywords": [
    "Electron",
    "Client Web"
  ],
  "author": "Flavio Moreira",
  "license": "GNU GENERAL PUBLIC LICENSE",
  "devDependencies": {
    "chai": "^3.4.1",
    "chai-as-promised": "^6.0.0",
    "check-for-leaks": "^1.2.0",
    "devtron": "^1.3.0",
    "electron": "^2.0.0",
    "electron-packager": "^8.7.2",
    "electron-squirrel-startup": "^1.0.0",
    "electron-winstaller": "^2.2.0",
    "grunt": "^1.0.2",
    "grunt-electron-installer": "^2.1.0",
    "husky": "^0.14.3",
    "mocha": "^3.1.0",
    "npm-run-all": "^4.0.2",
    "request": "^2.70.0",
    "rimraf": "^2.5.2",
    "signcode": "^0.5.0",
    "spectron": "~3.6.0",
    "standard": "^8.2.0"
  },
  "dependencies": {
    "electron-settings": "^3.0.7",
    "electron-shortcut-normalizer": "^1.0.0",
    "glob": "^7.1.0",
    "highlight.js": "^9.3.0",
    "jquery": "^3.3.1",
    "jquery-ui": "^1.11.1",
    "remote": "^0.2.6",
    "window": "^4.2.5"
  },
  "standard": {
    "env": {
      "mocha": true
    }
  }
}
